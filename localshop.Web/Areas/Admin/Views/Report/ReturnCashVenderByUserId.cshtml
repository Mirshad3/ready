
@using localshop.Core.Common
@model IEnumerable<InvoiceTotalViewModel>

@{
    Layout = null;
    ViewBag.Title = "ReturnCashVenderByUserId";
}

<!DOCTYPE html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>localshop - @(ViewBag.Title ?? "Electronic store")</title>
    <meta name="robots" content="noindex, follow" />
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- CSS
    ============================================ -->
    <!-- Bootstrap CSS -->
    @Styles.Render("~/content/client/css/vendor")

    <!-- Icon Font CSS -->
    <link rel="stylesheet" href="~/Assets/client/css/vendor/line-awesome.css">
    <link rel="stylesheet" href="~/Assets/client/css/vendor/themify.css">

    <!-- Plugins CSS -->
    @Styles.Render("~/content/client/css/tracking")

    <!-- Main CSS -->
    @Styles.Render("~/content/client/css/app")
</head>
<body>
    <div class="row">
        <div class="col-12">
            <div class="card-box overflow-auto">
                <table id="datatable" class="table table-striped table-bordered nowrap">
                    <thead>
                        <tr>

                            <th>Date</th>
                            <th>Total</th>
                            <th>Sub Total</th>
                            <th>Deduction %</th>
                            <th>Deduction Amount</th>
                            <th>Tex</th>
                            <th>Return Amount</th>
                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var o in Model)
                        {
                            <tr data-orderid="@o.StartDate">
                                <td>@o.StartDate</td>
                                <td>@o.Total</td>
                                <td>@o.SubTotal</td>
                                <td>@o.DetuctionPersontage</td>
                                <td>@o.Detuction</td>
                                <td>@o.Tex</td>
                                <td>@o.ReturnTotal</td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div> <!-- end row -->

</body>
</html>